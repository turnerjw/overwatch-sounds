{
  "name": "overwatch-sounds",
  "displayName": "Overwatch Sounds",
  "description": "Hanzo please switch",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "overwatch_sounds.enable",
        "title": "Overwatch Sounds: Enable"
      },
      {
        "command": "overwatch_sounds.disable",
        "title": "Overwatch Sounds: Disable"
      },
      {
        "command": "overwatch_sounds.volumeUp",
        "title": "Overwatch Sounds: Volume Up"
      },
      {
        "command": "overwatch_sounds.volumeDown",
        "title": "Overwatch Sounds: Volume Down"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash.debounce": "^4.0.6",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "play-sound": "^1.1.3"
  }
}
